{
  "name": "ghas-licence-clean",
  "version": "1.0.0",
  "description": "A GitHub Action that helps clean up GHAS Licences",
  "main": "main.ts",
  "scripts": {
    "build": "tsc && ncc build ./src/main.ts -o dist",
    "prettier:write": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint --fix . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install",
    "local": "yarn run build && act --job dispatch --env-file ./.env --eventpath .github/workflows/regression/payload.event --secret-file my.secrets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/artifact": "^4.0.0",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@octokit/action": "^8.0.4",
    "@octokit/plugin-retry": "^8.0.3",
    "@octokit/plugin-throttling": "^11.0.3",
    "@vercel/ncc": "^0.38.4",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.8",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": "20"
  }
}
